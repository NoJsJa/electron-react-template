{
  "name": "electron-react-template",
  "description": "[ electron3 + react16 + webpack4 + react-router4 ] development template",
  "version": "v1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/NoJsJa/electronux.git"
  },
  "author": {
    "name": "NoJsJa",
    "email": "yangwei020154@gmail.com"
  },
  "scripts": {
    "start": "concurrently \"npm run start-dev\"",
    "start-dll": "cross-env NODE_ENV=development webpack --config webpack.dll.config.js",
    "start-dev": "npm run start-dll && cross-env NODE_ENV=development webpack-dev-server",
    "build": "npm run start-dll && cross-env NODE_ENV=production webpack --config webpack.prod.config.js"
  },
  "keywords": [
    "electron",
    "react",
    "mobx",
    "react-router",
    "webpack4"
  ],
  "license": "",
  "nodemonConfig": {
    "ignore": [
      "resources/*",
      "node_modules/*",
      "dist/*",
      "build/*",
      "app/stores/*",
      "app/styles/*",
      "app/services/shell/*",
      "app/configure/view.conf",
      "app/views/*",
      "app/App.js",
      "app/main.js",
      "app/index.js",
      "electron-builder.yml"
    ],
    "delay": "1000"
  },
  "dependencies": {
    "mobx": "^4.4.1",
    "mobx-react": "^5.2.8",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-hot-loader": "^4.3.8",
    "react-router": "^3.2.4",
    "semantic-ui-css": "^2.4.0",
    "axios": "^0.19.2",
    "lowdb": "^1.0.0",
    "ping": "^0.2.2",
    "qs": "^6.9.3",
    "semantic-ui-react": "^0.82.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "source-map-support": "^0.5.9",
    "style-loader": "^0.21.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.1.8"
  }
}
